# include<stdio.h>
int main ()
{
    char a[100], b[100];
    int i=0,j=0;
    printf ("Enter string without space:");
    scanf ("%s",a);
    while (a[i]!='\0')
    {
        b[j] = a[i];
        i++,j++;
    }
    printf("\nBefore copying: %s",a);
    printf ("\nAfter copying:%s",b);
    return 0;
}
-------------------------------------------
Output:
Enter string without space:Goodnight
Before copying: Goodnight
After copying:Goodnight
-------------------------------------------
