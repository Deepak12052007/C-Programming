#include<stdio.h>
int main ()
{
   int n,i,sum=0;
   printf("Enter the number:\n");
   scanf("%d",&n);
   for(i=1;i<=n;i++)
   {
       sum=sum+i;
   }
   printf("Sum of the natural number:%d",sum);
    return 0;
}
------------------------------------------------
Output
Enter the number:5
Sum of the natural number:15
