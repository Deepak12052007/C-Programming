# include<stdio.h>
int main()
{
    int num,a,b;
    printf("Enter a number:");
    scanf("%d",&num);
    a=num%5;
    b=num%11;
    if(a&&b==0)
    {
        printf("Given number is divisible by 5 and 11");
    }
    else{
        printf("Given number is not divisible by 5 and 11");
    }
    return 0;
}
---------------------------------------------
Enter a number:5
Given number is not divisible by 5 and 11
